SCRIPT_DIR=/home/vagozino/.config/i3blocks/scripts

[current_track]
command=$SCRIPT_DIR/current_track
interval=once
signal=10

[disk]
command=$SCRIPT_DIR/disk
interval=60

[volume-pulseaudio]
command=$SCRIPT_DIR/volume-pulseaudio
interval=persist
SUBSCRIBE=1
USE_ALSA_NAME=1

[time]
command=$SCRIPT_DIR/time
interval=50
#TZ_FILE=~/.tz
STRFTIME_FORMAT=%a %d %b %H:%M
TZONES=$DEFAULT_TZ, Dab
TZ_LABELS=,Dab


